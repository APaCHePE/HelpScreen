<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="#5E040404"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HelpRentabilidad">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent=".15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView21"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="24dp"
            android:tint="@color/blanco"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent=".37"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias=".5"
            app:layout_constraintWidth_percent=".15"
            app:srcCompat="@android:drawable/ic_delete" />

        <TextView
            android:id="@+id/textView42"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:autoSizeTextType="uniform"
            android:text="Retrocede a la sección anterior"
            android:textAlignment="textStart"
            android:textColor="@color/blanco"
            app:layout_constraintHeight_percent=".4"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias=".5"
            app:layout_constraintEnd_toEndOf="@+id/imageView21"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/imageView21"
            app:layout_constraintTop_toBottomOf="@+id/imageView21"
            app:layout_constraintWidth_percent=".5" />

        <TextView
            android:id="@+id/textView49"
            android:layout_width="0dp"
            android:layout_height="2dp"
            android:background="@color/blanco"
            app:layout_constraintBottom_toBottomOf="@+id/textView42"
            app:layout_constraintEnd_toStartOf="@+id/textView42"
            app:layout_constraintStart_toEndOf="@+id/textView63"
            app:layout_constraintTop_toTopOf="@+id/textView42"
            app:layout_constraintWidth_percent=".3" />

        <TextView
            android:id="@+id/textView63"
            android:layout_width="2dp"
            android:layout_height="0dp"
            android:background="@color/blanco"
            app:layout_constraintBottom_toTopOf="@+id/textView49"
            app:layout_constraintEnd_toStartOf="@+id/imageView21"
            app:layout_constraintHeight_percent=".3"
            app:layout_constraintHorizontal_bias="0.174"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView21"
            app:layout_constraintVertical_bias="1.0" />
    </androidx.constraintlayout.widget.ConstraintLayout>


    <TextView
        android:id="@+id/textView44"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:autoSizeTextType="uniform"
        android:text="Accede al calendario Bode+ para poder elegir el día o rango de días de tu reporte"
        android:textColor="@color/blanco"
        app:layout_constraintBottom_toTopOf="@+id/textView65"
        app:layout_constraintHeight_percent=".1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2"
        app:layout_constraintVertical_bias="0.791"
        app:layout_constraintWidth_percent=".7" />

    <TextView
        android:id="@+id/textView67"
        android:layout_width="2dp"
        android:layout_height="0dp"
        android:background="@color/blanco"
        app:layout_constraintBottom_toTopOf="@+id/textView58"
        app:layout_constraintHeight_percent=".1"
        app:layout_constraintStart_toEndOf="@+id/textView58" />

    <TextView
        android:id="@+id/textView58"
        android:layout_width="0dp"
        android:layout_height="2dp"
        android:background="@color/blanco"
        app:layout_constraintBottom_toBottomOf="@+id/textView44"
        app:layout_constraintStart_toEndOf="@+id/textView44"
        app:layout_constraintTop_toTopOf="@+id/textView44"
        app:layout_constraintWidth_percent=".120" />

    <TextView
        android:id="@+id/textView65"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/textView64"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2"
        app:layout_constraintVertical_bias=".4" />

    <TextView
        android:id="@+id/textView45"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:autoSizeTextType="uniform"
        android:text="Accede al carrito de compras"

        android:textAlignment="center"
        android:textColor="@color/blanco"
        app:layout_constraintBottom_toTopOf="@+id/textView61"
        app:layout_constraintEnd_toEndOf="@+id/textView48"
        app:layout_constraintStart_toStartOf="@+id/textView47"
        app:layout_constraintWidth_percent=".4" />

    <TextView
        android:id="@+id/textView61"
        android:layout_width="2dp"
        android:layout_height="0dp"
        android:background="@color/blanco"
        app:layout_constraintBottom_toTopOf="@+id/textView64"
        app:layout_constraintEnd_toStartOf="@+id/textView48"
        app:layout_constraintHeight_percent=".12"
        app:layout_constraintStart_toEndOf="@+id/textView47"
        app:layout_constraintVertical_bias="0.782" />

    <TextView
        android:id="@+id/textView66"
        android:layout_width="2dp"
        android:layout_height="0dp"
        android:background="@color/blanco"
        app:layout_constraintBottom_toTopOf="@+id/textView64"
        app:layout_constraintEnd_toEndOf="@+id/textView47"
        app:layout_constraintHeight_percent=".06"
        app:layout_constraintStart_toStartOf="@+id/textView47" />

    <TextView
        android:id="@+id/textView48"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_marginEnd="92dp"
        android:autoSizeTextType="uniform"
        android:text="Accede a configuración"
        android:textAlignment="center"
        android:textColor="@color/blanco"
        app:layout_constraintBottom_toTopOf="@+id/textView62"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintWidth_percent=".3" />

    <TextView
        android:id="@+id/textView47"
        android:layout_width="0dp"
        android:layout_height="25dp"
        android:layout_marginStart="16dp"
        android:autoSizeTextType="uniform"
        android:text="Ir a inicio"
        android:textColor="@color/blanco"
        app:layout_constraintBottom_toTopOf="@+id/textView66"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintWidth_percent=".2" />

    <TextView
        android:id="@+id/textView62"
        android:layout_width="1dp"
        android:layout_height="31dp"
        android:background="@color/blanco"
        app:layout_constraintBottom_toTopOf="@+id/textView64"
        app:layout_constraintEnd_toEndOf="@+id/textView48"
        app:layout_constraintHeight_percent=".04"
        app:layout_constraintHorizontal_bias="0.475"
        app:layout_constraintStart_toStartOf="@+id/textView48" />


    <TextView
        android:id="@+id/textView64"
        android:layout_width="match_parent"
        android:layout_height="70dp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>